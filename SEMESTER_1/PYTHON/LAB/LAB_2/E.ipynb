   ],
   "source": [
    "get=int(input(\"Enter the number of terms: \"))\n",
    "i,u=1,0\n",
    "while(i<=get):\n",
    "    m=int(input(f\"Enter the {i} number: \"))\n",
    "    u+=m\n",
    "    i+=1\n",
    "    if (i>get):\n",
    "        break\n",
    "print(\"Average: \",u//get)"
   ]
  }
